property_value: RO:0002175 NCBITaxon:9608 {source="http://www.ncbi.nlm.nih.gov/pubmed/14803383", source="https://sourceforge.net/p/geneontology/ontology-requests/10639"}
property_value: RO:0002175 NCBITaxon:9681 {source="https://sourceforge.net/p/geneontology/ontology-requests/10639", source="PMID:6997222 "}
